{
	"name": "utils-extension",
	"displayName": "utils-extension",
	"description": "this extension will help developers create tests, mocks, components and general utils on Salesforce.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "utils-extension.createFile",
				"title": "Triscal_Dev: Create File"
			},
			{
				"command": "utils-extension.createFolder",
				"title": "Triscal_Dev: Create Folder"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "utils-extension.createFile",
					"when": "1==1"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.56.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}